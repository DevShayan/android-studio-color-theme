	"tokenColors": [
		{
			"name": "Generic: Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Generic: Documentation Comments",
			"scope": [
				"comment.documentation",
				"comment.block.javadoc",
				"comment.line.double-slash.documentation"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#629755"
			}
		},
		{
			"name": "Generic: Keywords",
			"scope": [
				"keyword",
				"storage",
				"keyword.declaration",
				"variable.language",
				"keyword.operator.instanceof",
				"support.function.builtin.shell"
			],
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"name": "Generic: Variables (inside class)",
			"scope": [
				"variable.other.definition",
				"variable.other.object.property",
				"support.variable"
			],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"name": "Generic: Used Function Names",
			"scope": [
				"meta.method.identifier",
				"entity.name.function.dart",
				"entity.name.function.definition",
				"entity.name.function.js"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FFC66D"
			}
		},
		{
			"name": "Generic: Numbers",
			"scope": [
				"constant.numeric",
				"constant.other.color.rgb-value.hex"
			],
			"settings": {
				"foreground": "#6897BB"
			}
		},
		{
			"name": "Generic: Strings",
			"scope": [
				"string.quoted.single",
				"string.quoted.double",
				"string.quoted.triple",
				"string.quoted.other",
				"string.interpolated",
				"string.template"
			],
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"name": "Generic: Escape Sequences: valid",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"name": "Generic: Tags + Tags Punctuation",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#e8bf6a"
			}
		},
		{
			"name": "Generic: Full Tag Block",
			"scope": [
				"meta.tag"
			],
			"settings": {
				"foreground": "#bababa"
			}
		},
		{
			"name": "Generic: Punctuation Colored",
			"scope": [
				"punctuation.terminator",
				"punctuation.separator.delimiter",
				"punctuation.separator.array",
				"punctuation.separator.dictionary",
				"punctuation.comma",
				"punctuation.separator.comma.js",
				"punctuation.separator.list.comma.css"
			],
			"settings": {
				"foreground": "#CC7832"
			}
		},
		{
			"name": "Generic: Constants (true, false)",
			"scope": [
				"constant.language"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#CC7832"
			}
		},
		{
			"name": "Java: Javadoc Comments: @param",
			"scope": [
				"keyword.other.documentation.javadoc"
			],
			"settings": {
				"fontStyle": "italic bold underline",
				"foreground": "#629755"
			}
		},
		{
			"name": "Java: Header",
			"scope": [
				"storage.modifier.import",
				"storage.modifier.package"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "Java: Annotations",
			"scope": [
				"storage.type.annotation",
				"punctuation.definition.annotation"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#BBB529"
			}
		},
		{
			"name": "Java: Defaluts",
			"scope": [
				"punctuation.definition.parameters",
				"keyword.control.ternary",
				"keyword.operator",
				"storage.type.generic.java",
				"storage.type.java",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"name": "C++: Header",
			"scope": [
				"punctuation.definition.directive",
				"keyword.control.directive"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#BBB529"
			}
		},
		{
			"name": "C++: Variables (inside class)",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"name": "XML: Namespace (before :)",
			"scope": [
				"entity.other.attribute-name.namespace"
			],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"name": "XML: Preprocessor Tag",
			"scope": [
				"entity.name.tag.xml"
			],
			"settings": {
				"foreground": "#bababa"
			}
		},
		{
			"name": "JSON: Property Names",
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#9876AA"
			}
		},
		{
			"name": "HTML: Strings",
			"scope": [
				"string.quoted.double.html"
			],
			"settings": {
				"foreground": "#a5c261"
			}
		},
		{
			"name": "CSS: Properties",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#bababa"
			}
		},
		{
			"name": "CSS: Values",
			"scope": [
				"keyword.other.unit",
				"support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#a5c261"
			}
		},
		{
			"name": "CSS: Attributes",
			"scope": [
				"entity.other.attribute-name.class",
				"entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element",
				"entity.other.attribute-name.css"
			],
			"settings": {
				"foreground": "#e8bf6a"
			}
		},
		{
			"name": "CSS: important",
			"scope": [
				"keyword.other.important.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cc7832"
			}
		},
		{
			"name": "CSS: Logical",
			"scope": [
				"keyword.operator.logical.only.media.css",
				"keyword.operator.logical.and.media.css"
			],
			"settings": {
				"foreground": "#e8bf6a"
			}
		},
		{
			"name": "CSS: After @media",
			"scope": [
				"support.constant.media.css"
			],
			"settings": {
				"foreground": "#a5c261"
			}
		},
		{
			"name": "CSS: Media Property",
			"scope": [
				"support.type.property-name.media.css"
			],
			"settings": {
				"foreground": "#e8bf6a"
			}
		},
		{
			"name": "CSS: Vendored Media Property",
			"scope": [
				"support.type.vendored.property-name.media.css"
			],
			"settings": {
				"foreground": "#bababa"
			}
		},
		{
			"name": "CSS: Defaults",
			"scope": [
				"punctuation.definition.entity.css"
			],
			"settings": {
				"foreground": "#a9b7c6"
			}
		},
		{
			"name": "CSS: Function call",
			"scope": [
				"support.function"
			],
			"settings": {
				"foreground": "#e8bf6a"
			}
		},
		{
			"name": "CSS: Parameter variables",
			"scope": [
				"variable.parameter.misc.css"
			],
			"settings": {
				"foreground": "#e8bf6a"
			}
		},
		{
			"name": "JS: Global Variables",
			"scope": [
				"variable.other.constant.js",
				"variable.other.object.js"
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#9876aa"
			}
		},
		{
			"name": "Markdown: Punctuation",
			"scope": [
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.bold.markdown"
			],
			"settings": {
				"foreground": "#cc7832"
			}
		},
		{
			"name": "Markdown: Heading",
			"scope": [
				"markup.heading.markdown"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9876aa"
			}
		},
		{
			"name": "Markdown: link line",
			"scope": [
				"meta.image.inline.markdown"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown: Link Description",
			"scope": [
				"punctuation.definition.link",
				"string.other.link.description",
				"string.other.link.title"
			],
			"settings": {
				"foreground": "#287bde"
			}
		},
		{
			"name": "Markdown: Link",
			"scope": [
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#ffc66d"
			}
		},
		{
			"name": "Markdown: Link Bracket",
			"scope": [
				"punctuation.definition.metadata.markdown"
			],
			"settings": {
				"foreground": "#6a8759"
			}
		},
		{
			"name": "Bash: Defaults",
			"scope": [
				"variable.other.normal.shell"
			],
			"settings": {
				"foreground": "#A9B7C6"
			}
		}
	]
}
