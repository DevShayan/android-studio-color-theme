	"tokenColors": [
		{
			"name": "Generic: Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#7a7e85"
			}
		},
		{
			"name": "Generic: Comments - Documentation",
			"scope": [
				"comment.block.javadoc.java",
				"comment.block.documentation.cpp",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5f826b"
			}
		},
		{
			"name": "Generic: Keywords",
			"scope": [
				"keyword",
				"storage",
				"keyword.declaration",
				"variable.language",
				"keyword.operator.instanceof",
				"keyword.operator.delete",
				"keyword.other.important.css",
				"keyword.operator.logical.only.media.css",
				"keyword.operator.logical.and.media.css"
			],
			"settings": {
				"foreground": "#cf8e6d"
			}
		},
		{
			"name": "Generic: Variables (inside class)",
			"scope": [
				"variable.other.definition",
				"variable.other.object.property",
				"support.variable"
			],
			"settings": {
				"foreground": "#c77dbb"
			}
		},
		{
			"name": "Generic: Function Names - Declaration",
			"scope": [
				"meta.method.identifier.java",
				"entity.name.function.dart",
				"entity.name.function.definition.cpp",
				"entity.name.function.definition.special.constructor.cpp",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#56a8f5"
			}
		},
		{
			"name": "Generic: Numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#2aacbb"
			}
		},
		{
			"name": "Generic: Strings",
			"scope": [
				"string.quoted.single",
				"string.quoted.double",
				"string.quoted.triple",
				"string.quoted.other",
				"string.interpolated",
				"string.template",
				"string.other.link.description.title.markdown"
			],
			"settings": {
				"foreground": "#6aab73"
			}
		},
		{
			"name": "Generic: Escape Sequences: valid",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#cf8e6d"
			}
		},
		{
			"name": "Generic: Tags, Tag Punctuation",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#d5b778"
			}
		},
		{
			"name": "Generic: Full Tag Block",
			"scope": [
				"meta.tag"
			],
			"settings": {
				"foreground": "#bababa"
			}
		},
		{
			"name": "Generic: Constants (true, false)",
			"scope": [
				"constant.language"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#cf8e6d"
			}
		},
		{
			"name": "Generic: Defaults",
			"scope": [
				"punctuation.separator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#bcbec4"
			}
		},
		{
			"name": "Java: Javadoc Tags: @param",
			"scope": [
				"keyword.other.documentation.javadoc"
			],
			"settings": {
				"foreground": "#67a37c"
			}
		},
		{
			"name": "Java: Annotations",
			"scope": [
				"storage.type.annotation",
				"punctuation.definition.annotation"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#b3ae60"
			}
		},
		{
			"name": "Java: Defaluts",
			"scope": [
				"keyword.control.ternary",
				"keyword.operator",
				"storage.type.generic",
				"storage.type.java",
				"variable.parameter",
				"storage.modifier.import",
				"storage.modifier.package",
				"storage.type.object.array.java"
			],
			"settings": {
				"foreground": "#bcbec4"
			}
		},
		{
			"name": "C++: Header",
			"scope": [
				"punctuation.definition.directive",
				"keyword.control.directive"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#b3ae60"
