			"name": "C++: Variables (inside class)",
			"scope": [
				"variable.other.property.cpp"
			],
			"settings": {
				"foreground": "#9373a5"
			}
		},
		{
			"name": "C++: Class Name, namespace",
			"scope": [
				"entity.name.type.class.cpp",
				"entity.name.type.struct.cpp",
				"entity.name.namespace.cpp",
